{
  "name": "nitsan/ns-twitter",
  "type": "typo3-cms-extension",
  "description": "Twitter extension brings all your recent tweets to your TYPO3 site. Demo: https://demo.t3terminal.com/t3t-extensions/twitter Pro version: You can download PRO version for more-features & free-support at https://t3terminal.com/twitter-display-plugin-free/",
  "homepage": "https://extensions.typo3.org/extension/ns_twitter/",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "twitter",
    "NITSAN"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_twitter/issues"
  },
  "require": {
    "typo3/cms-core": ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NsTwitter\\": "Classes/"
    }
  },
  "replace": {
      "nitsan/ns-twitter": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/public",
        "extension-key": "ns_twitter"
    }
  }
}
